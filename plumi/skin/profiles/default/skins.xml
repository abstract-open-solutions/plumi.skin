<?xml version="1.0"?>
<object name="portal_skins"
	allow_any="False"
	cookie_persistence="False"
	default_skin="plumi.skin.layer"
	request_varname="plone_skin">

  <object name="plumi.skin"
	  meta_type="Filesystem Directory View"
	  directory="plumi.skin.browser:browser_layer" />
  
  <skin-path name="plumi.skin.layer"
	     based-on="Plone Default">
    <layer name="plumi.skin"
	   insert-after="custom" />
  </skin-path>
</object>