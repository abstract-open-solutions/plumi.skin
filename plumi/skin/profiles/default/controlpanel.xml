<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="plumi.skin"
    purge="False">

    <configlet
        title="Plumi"
        action_id="plumi"
        appId="plumi.skin"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@plumi-settings"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
